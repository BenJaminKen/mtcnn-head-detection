  bottom: "fc5_1"
  top: "fc5_1"
}
layer {
  name: "fc6_1"
  type: "InnerProduct"
  bottom: "fc5_1"
  top: "fc6_1"
  param {
    lr_mult: 1
    decay_mult: 1
