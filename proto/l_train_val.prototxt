  param {
    lr_mult: 2
    decay_mult: 0
  }
  inner_product_param {
    num_output: 2
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}
layer {
  name: "loss_1"
  type: "EuclideanLoss"
  bottom: "fc6_1"
  bottom: "target_1"
  top: "loss_1"
}
