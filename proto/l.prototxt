######## concat ########
layer {
  name: "concat"
  type: "Concat"
  bottom: "conv3_1"
  bottom: "conv3_2"
  bottom: "conv3_3"
  bottom: "conv3_4"
  bottom: "conv3_5"
  top: "concat"
  concat_param {
    axis: 1
  }
}
layer {
  name: "fc4"
  type: "InnerProduct"
  bottom: "concat"
  top: "fc4"
  inner_product_param {
    num_output: 256
  }
}
layer {
  name: "prelu4"
  type: "PReLU"
  bottom: "fc4"
  top: "fc4"
}

######## net-1 output ########
layer {
  name: "fc5_1"
  type: "InnerProduct"
  bottom: "fc4"
  top: "fc5_1"
  inner_product_param {
    num_output: 64
  }
}
layer {
  name: "prelu5_1"
  type: "PReLU"
  bottom: "fc5_1"
  top: "fc5_1"
}

layer {
  name: "fc6_1"
