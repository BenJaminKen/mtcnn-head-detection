layer {
  name: "prelu3_3"
  type: "PReLU"
  bottom: "conv3_3"
  top: "conv3_3"
}

######## net-4 ########
layer {
  name: "conv1_4"
  type: "Convolution"
  bottom: "data_4"
  top: "conv1_4"
  convolution_param {
    num_output: 28
    kernel_size: 3
    stride: 1
  }
